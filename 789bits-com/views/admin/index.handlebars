
<!-- Botão para acionar modal -->
<input type="text" pattern="/[0-9]{2}\.?[0-9]{3}\.?[0-9]{3}\/?[0-9]{4}\-?[0-9]{2}/g" id="cnpj">
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalExemplo" id="consulta">
  Consulta CNPJ
</button>
<!-- Modal -->
<div class="modal fade" id="modalExemplo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="nome"></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Fechar">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        
        <h6><a id="logradouro"></a></h6>
        <h6>Nº: <a id="numero"></a></h6>
        
        <h6>BAIRRO: <a id="bairro"></a></h6>
        <h6>CEP: <a id="cep"> </a></h6>
        
        <h6>CIDADE: <a id="municipio"></a> - 
        <a id="uf"></a></h6>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
        <button type="button" class="btn btn-primary">Salvar mudanças</button>
      </div>
    </div>
  </div>
</div>