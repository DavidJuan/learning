{{#each contatos}}
    <div class="card mt-2">
        <div class="card-body">
            <h3>{{nome}}</h3>
            <h5>{{tipoContato}}&emsp;{{contato}}</h5>
            <h6>{{mensagem}}</h6>
            <small class="data date-{{id}}"></small>
        </div>
    </div>

    <script>
        document.querySelector('.date-{{id}}').innerHTML = new Date('{{date}}').toLocaleString();
    </script>

{{/each}}